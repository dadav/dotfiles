// Place your key bindings in this file to override the defaultsauto[]
[
    {
        "key": "alt+numpad5",
        "command": "flutter.hotReload"
    },
    {
        "key": "alt+numpad9",
        "command": "flutter.attach"
    },
    {
        "key": "alt+numpad4",
        "command": "flutter.hotRestart"
    },
    {
        "key": "alt+numpad_separator",
        "command": "editor.action.quickFix",
        "when": "editorHasCodeActionsProvider && editorTextFocus && !editorReadonly"
    },
    {
        "key": "ctrl+[Period]",
        "command": "-editor.action.quickFix",
        "when": "editorHasCodeActionsProvider && editorTextFocus && !editorReadonly"
    },
    {
        "key": "alt+numpad1",
        "command": "workbench.action.debug.run",
        "when": "debuggersAvailable && debugState != 'initializing'"
    },
    {
        "key": "ctrl+f5",
        "command": "-workbench.action.debug.run",
        "when": "debuggersAvailable && debugState != 'initializing'"
    },
    {
        "key": "alt+numpad2",
        "command": "workbench.action.debug.start",
        "when": "debuggersAvailable && debugState == 'inactive'"
    },
    {
        "key": "f5",
        "command": "-workbench.action.debug.start",
        "when": "debuggersAvailable && debugState == 'inactive'"
    },
    {
        "key": "ctrl+numpad3",
        "command": "editor.emmet.action.balanceOut"
    },
    {
        "key": "ctrl+numpad2",
        "command": "editor.emmet.action.wrapWithAbbreviation"
    },
    {
        "key": "ctrl+numpad6",
        "command": "editor.emmet.action.selectNextItem"
    },
    {
        "key": "ctrl+numpad4",
        "command": "editor.emmet.action.selectPrevItem"
    },
    {
        "key": "ctrl+numpad_multiply",
        "command": "editor.emmet.action.reflectCSSValue"
    },
    {
        "key": "ctrl+numpad1",
        "command": "editor.emmet.action.balanceIn"
    },
    {
        "key": "ctrl+numpad7",
        "command": "editor.action.commentLine",
        "when": "editorTextFocus && !editorReadonly"
    },
    {
        "key": "ctrl+shift+7",
        "command": "-editor.action.commentLine",
        "when": "editorTextFocus && !editorReadonly"
    },
    {
        "key": "shift+enter",
        "command": "findJump.activate",
        "when": "editorTextFocus"
    },
    {
        "key": "ctrl+numpad5",
        "command": "editor.emmet.action.balanceIn"
    }
]